<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.sdzee.hibernate.Utilisateur" table="Utilisateur">
        <id name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
        <property name="motDePasse" type="timestamp" column="mot_de_passe"/>
        <property name="pseudo" type="timestamp" column="pseudo"/>
        <property name="dateInscription" type="timestamp" column="date_inscription"/>
    </class>

</hibernate-mapping>